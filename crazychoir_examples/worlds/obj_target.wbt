Robot {
  name "$NAME"
  translation $X $Y $Z
  children [
    Solid {
      children [
        Shape {
          appearance PBRAppearance {
            baseColor 0 0 1
            transparency 0.1
            metalness 0
          }
          geometry Sphere {
            radius 0.1
          }
        }
      ]
    }
  ]
}